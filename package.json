{
  "name": "qinglong",
  "private": true,
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "scripts": {
    "build": "pnpm -r --filter='./packages/*' run build",
    "dev": "pnpm -r --parallel --filter='./packages/*' run dev"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.21",
    "cron-parser": "^4.2.1",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "qiniu": "^7.4.0",
    "typescript": "^4.6.2",
    "yorkie": "^2.0.0"
  },
  "engines": {
    "node": ">=12.22.0",
    "pnpm": ">= 7.2.1"
  },
  "packageManager": "pnpm@7.3.0",
  "dependencies": {
    "lodash-es": "^4.17.21"
  }
}
